{
  "name": "twitter-clone-react-nodejs",
  "version": "1.0.0",
  "description": "Twittter clone demo for my training",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "db:create": "sequelize-cli db:create",
    "db:reset": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "jayrick_gacayan",
  "keywords": [
    "node",
    "jwt",
    "express",
    "bcrypt",
    "authorization",
    "postgresql",
    "sequelize"
  ],
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-fileupload": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.19.0"
  },
  "devDependencies": {
    "concurrently": "^7.2.1",
    "nodemon": "^2.0.16",
    "sequelize-cli": "^6.4.1"
  }
}
